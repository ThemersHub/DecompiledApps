<?xml version="1.0" encoding="utf-8"?>
<selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:drawable="@drawable/scrubber_control_disabled_holo_light" />
    <item android:state_pressed="true" android:drawable="@drawable/scrubber_control_pressed_holo_light" />
    <item android:state_selected="true" android:drawable="@drawable/scrubber_control_pressed_holo_light" />
    <item android:state_focused="true" android:drawable="@drawable/scrubber_control_pressed_holo_light" />
    <item android:drawable="@drawable/scrubber_control_normal_holo_light" />
    <item android:state_checked="true">
        <shape>
            <size android:height="32.0dip" android:width="32.0dip" />
        </shape>
    </item>
</selector>