<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/bg">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="26.0dip" android:thickness="4.0dip">
            <solid android:color="#30ffffff" />
        </shape>
    </item>
    <item android:id="@id/progress">
        <rotate android:useLevel="false" android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:useLevel="true" android:innerRadius="26.0dip" android:thickness="4.0dip"
              xmlns:android="http://schemas.android.com/apk/res/android">
                <solid android:color="#ffffffff" />
            </shape>
        </rotate>
    </item>
</layer-list>