<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/prefs_title" android:key="docs_preference_screen"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference android:title="@string/prefs_cache_size" android:key="shared_preferences.cache_size" android:order="@integer/prefs_cache_size_order" />
    <com.google.android.libraries.docs.accessibility.AccessiblePreferenceCategory android:title="@string/prefs_encryption_category" android:key="encryption" android:order="@integer/prefs_encryption_category_order">
        <SwitchPreference android:title="@string/prefs_enable_pin_encryption_title" android:key="enable_pin_encryption" android:summary="@string/prefs_enable_pin_encryption_summary" android:defaultValue="true" />
        <SwitchPreference android:title="@string/prefs_streaming_decryption_title" android:key="streaming_decryption" android:summary="@string/prefs_streaming_decryption_summary" android:defaultValue="false" />
    </com.google.android.libraries.docs.accessibility.AccessiblePreferenceCategory>
    <com.google.android.libraries.docs.accessibility.AccessiblePreferenceCategory android:title="@string/prefs_storage_category_title" android:key="storage" android:order="@integer/prefs_storage_category_order">
        <Preference android:layout="@layout/storage_preference_row" android:title="@string/storage_used" android:key="storage_add" android:summary="@string/storage_upgrade" />
    </com.google.android.libraries.docs.accessibility.AccessiblePreferenceCategory>
    <com.google.android.libraries.docs.accessibility.AccessiblePreferenceCategory android:title="@string/prefs_devtools_category_title" android:key="developer_tools" android:order="900">
        <Preference android:title="@string/prefs_devtools_flags_title" android:key="prefs_flags" android:summary="@string/prefs_devtools_flags_summary" />
        <Preference android:title="@string/prefs_devtools_impressions_title" android:key="prefs_impressions" android:summary="@string/prefs_devtools_impressions_summary" />
    </com.google.android.libraries.docs.accessibility.AccessiblePreferenceCategory>
</PreferenceScreen>