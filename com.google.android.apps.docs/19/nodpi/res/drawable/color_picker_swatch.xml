<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item id="@+id/highlight">
        <shape android:gravity="center" android:shape="oval" />
    </item>
    <item id="@+id/inset">
        <shape android:gravity="center" android:shape="oval">
            <stroke android:width="4.0dip" android:color="@android:color/transparent" />
        </shape>
    </item>
</layer-list>