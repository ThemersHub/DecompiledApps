            <keyframe android:value="1.8" android:interpolator="@android:interpolator/fast_out_slow_in" android:fraction="1.0" />
        </propertyValuesHolder>
        <propertyValuesHolder android:valueType="floatType" android:propertyName="scaleY">
            <keyframe android:value="1" android:fraction="0.0" />
            <keyframe android:value="1" android:fraction="0.57" />
            <keyframe android:value="1.8" android:interpolator="@android:interpolator/fast_out_slow_in" android:fraction="1.0" />
        </propertyValuesHolder>
