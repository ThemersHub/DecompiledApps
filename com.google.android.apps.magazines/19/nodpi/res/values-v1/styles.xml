<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Text.HeaderTitle" parent="@style/TextDisplay.L" />
    <style name="Text.Label" parent="@style/TextBody.XS">
        <item name="android:maxLines">1</item>
        <item name="android:textAllCaps">true</item>
        <item name="customFont">BOLD_SANS</item>
    </style>
    <style name="TextBody" parent="@style/TextN2">
        <item name="android:textColor">@color/text_color_primary</item>
        <item name="customFont">REGULAR_SANS</item>
    </style>
    <style name="TextBody.L" parent="@style/TextBody">
        <item name="android:textSize">@dimen/text_body_L</item>
    </style>
    <style name="TextBody.XS" parent="@style/TextBody">
        <item name="android:textSize">@dimen/text_body_XS</item>
        <item name="android:textColor">@color/text_color_secondary</item>
    </style>
</resources>
