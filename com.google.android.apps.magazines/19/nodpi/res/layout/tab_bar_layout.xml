<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" android:id="@id/tab_bar_layout" android:focusable="true" style="@style/BottomBar"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:ns="http://schemas.android.com/apk/res-auto">
    <LinearLayout android:gravity="center" android:orientation="horizontal" android:background="@color/bottom_bar_background_color" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout android:id="@id/tab_for_you" style="@style/BottomBarButton">
            <android.support.v7.widget.AppCompatImageView ns:srcCompat="@drawable/ic_for_you" style="@style/BottomBarButtonIcon" />
            <com.google.apps.dots.android.modules.widgets.bound.NSTextView android:text="@string/for_you_title" style="@style/BottomBarButtonText" />
        </com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout>
        <com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout android:id="@id/tab_headlines" style="@style/BottomBarButton">
            <android.support.v7.widget.AppCompatImageView ns:srcCompat="@drawable/quantum_ic_language_grey600_24" style="@style/BottomBarButtonIcon" />
            <com.google.apps.dots.android.modules.widgets.bound.NSTextView android:text="@string/headlines_title" style="@style/BottomBarButtonText" />
        </com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout>
        <com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout android:id="@id/tab_video" android:visibility="gone" style="@style/BottomBarButton">
            <android.support.v7.widget.AppCompatImageView android:src="@drawable/quantum_ic_play_arrow_white_24" style="@style/BottomBarButtonIcon" />
            <com.google.apps.dots.android.modules.widgets.bound.NSTextView android:text="@string/video" style="@style/BottomBarButtonText" />
        </com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout>
        <com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout android:id="@id/tab_following" style="@style/BottomBarButton">
            <android.support.v7.widget.AppCompatImageView ns:srcCompat="@drawable/quantum_gm_ic_grade_vd_theme_24" style="@style/BottomBarButtonIcon" />
            <com.google.apps.dots.android.modules.widgets.bound.NSTextView android:text="@string/favorites_title" style="@style/BottomBarButtonText" />
        </com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout>
        <com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout android:id="@id/tab_store" style="@style/BottomBarButton">
            <android.support.v7.widget.AppCompatImageView ns:srcCompat="@drawable/ic_newsstand_24" style="@style/BottomBarButtonIcon" />
            <com.google.apps.dots.android.modules.widgets.bound.NSTextView android:text="@string/native_store_title" style="@style/BottomBarButtonText" />
        </com.google.apps.dots.android.modules.widgets.bound.viewgroup.NSLinearLayout>
    </LinearLayout>
</LinearLayout>