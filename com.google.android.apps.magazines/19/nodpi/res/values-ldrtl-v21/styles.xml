<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AudioSeekBar">
        <item name="android:focusable">true</item>
        <item name="android:maxHeight">4.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/audio_progress</item>
        <item name="android:progressDrawable">@drawable/audio_progress</item>
        <item name="android:minHeight">4.0dip</item>
        <item name="android:thumbOffset">16.0dip</item>
        <item name="android:mirrorForRtl">true</item>
        <item name="android:splitTrack">false</item>
    </style>
    <style name="CardArticleItemText.Kicker" parent="@style/CardText">
        <item name="android:textSize">@dimen/text_body_S</item>
        <item name="android:paddingTop">0.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:paddingStart">@dimen/card_inner_padding_fullbleed</item>
        <item name="android:paddingEnd">@dimen/card_inner_padding_fullbleed</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="bindText">@id/CardArticleItem_kicker</item>
        <item name="bindTextColor">@id/CardArticleItem_kickerTextColor</item>
        <item name="bindVisibility">@id/CardArticleItem_kicker</item>
        <item name="customFont">MEDIUM_SANS</item>
    </style>
    <style name="CardCarousel.Cluster" parent="@style/NSCardView.Cluster">
        <item name="android:paddingStart">0.0dip</item>
        <item name="android:paddingEnd">0.0dip</item>
        <item name="cardBackgroundColor">@color/card_carousel_card_background</item>
        <item name="cardCornerRadius">0.0dip</item>
        <item name="cardElevation">@dimen/card_article_item_content_elevation</item>
        <item name="cardInsetTop">0.0dip</item>
        <item name="cardInsetBottom">0.0dip</item>
        <item name="disableShadow">true</item>
        <item name="bindPaddingTopResId">@null</item>
        <item name="bindPaddingBottomResId">@null</item>
    </style>
    <style name="CardFooterActionIcon">
        <item name="android:background">@drawable/card_footer_button_bg</item>
        <item name="android:padding">@dimen/card_footer_button_padding</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">@dimen/card_footer_button_size</item>
        <item name="android:layout_height">@dimen/card_footer_button_size</item>
        <item name="android:elevation">@dimen/card_footer_button_elevation</item>
    </style>
    <style name="CardFooterActionIcon.NoBackground" parent="@style/CardFooterActionIcon">
        <item name="android:background">@drawable/tab_background</item>
        <item name="android:padding">@dimen/card_footer_button_padding_no_bg</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">@dimen/card_footer_button_size</item>
        <item name="android:layout_height">@dimen/card_footer_button_size</item>
        <item name="android:scaleType">center</item>
        <item name="android:elevation">0.0dip</item>
    </style>
    <style name="CardSourceListItemSubscribeButton">
        <item name="android:background">@drawable/tab_background</item>
    </style>
    <style name="FloatingActionButtonBranded">
        <item name="android:tint">@null</item>
        <item name="android:elevation">4.0dip</item>
        <item name="rippleColor">@color/google_fab_branded_ripple_color</item>
        <item name="srcCompat">@drawable/coloredicons_cic_create_36</item>
        <item name="fabSize">normal</item>
        <item name="maxImageSize">@dimen/google_fab_branded_image_size</item>
        <item name="backgroundTint">@color/white</item>
    </style>
    <style name="LandingPageIconLabel" parent="@style/Text.Label">
        <item name="android:textSize">@dimen/text_body_M</item>
        <item name="android:gravity">center</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:paddingStart">@dimen/card_inner_content_padding</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="customFont">MEDIUM_SANS</item>
    </style>
    <style name="NSCardView.Cluster" parent="@style/NSCardView">
        <item name="cardBackgroundColor">@android:color/transparent</item>
        <item name="cardCornerRadius">0.0dip</item>
        <item name="layout_flmGridInsetStart">@dimen/collection_view_padding_fullbleed</item>
        <item name="layout_flmGridInsetEnd">@dimen/collection_view_padding_fullbleed</item>
        <item name="layoutSpanStyle">@style/CardSpan.Cluster</item>
        <item name="disableShadow">true</item>
    </style>
    <style name="NSCardView.RoundedButton" parent="@style/NSCardView">
        <item name="android:layout_height">@dimen/rounded_button_card_height</item>
        <item name="android:foreground">@drawable/card_overlay_lighten</item>
        <item name="android:paddingStart">@dimen/rounded_button_horizontal_padding</item>
        <item name="android:paddingEnd">@dimen/rounded_button_horizontal_padding</item>
        <item name="cardBackgroundColor">@color/app_color_material</item>
        <item name="cardCornerRadius">@dimen/rounded_button_corner_radius</item>
    </style>
    <style name="NSToolbarStyle.Transparent" parent="@style/NSToolbarStyle">
        <item name="android:background">@color/transparent_toolbar_bg_ripple_fix</item>
    </style>
    <style name="PlayMediumPlainText" parent="@style/PlayPlainText">
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="ShelfHeaderTitle.Hero" parent="@style/ShelfHeaderTitle">
        <item name="android:textSize">@dimen/shelf_header_subtitle</item>
        <item name="android:textStyle">bold</item>
        <item name="android:maxLines">1</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.05</item>
        <item name="customFont">BOLD_SANS</item>
    </style>
    <style name="Text.Label" parent="@style/TextBody.XS">
        <item name="android:maxLines">1</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.075</item>
        <item name="customFont">BOLD_SANS</item>
    </style>
</resources>
