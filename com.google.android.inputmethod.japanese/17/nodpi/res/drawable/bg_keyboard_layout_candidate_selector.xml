<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <selector>
            <item android:state_selected="true" android:drawable="@drawable/ic_keyboard_layout_candidate_selected" />
        </selector>
    </item>
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="@color/tutorial_press_color" />
                </shape>
            </item>
        </selector>
    </item>
</layer-list>