<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@id/softkey_template_for_tv_ja" layout="@layout/softkey_tv_input_ja" popup_timing="@string/qwerty_popup_timing" content_description="$desc$">
            <action type="PRESS" data="$press_data$" keycode="PLAIN_TEXT" intention="DECODE" popup_label="$press_data$" />
            <action type="DOUBLE_TAP" data="$double_tap_data$" keycode="PLAIN_TEXT" intention="DECODE" popup_label="$double_tap_data$" popup_layout="$double_tap_data$" />
            <label location="@id/label" value="$press_data$" />
        </softkey_template>
    </softkeys>
</framework>