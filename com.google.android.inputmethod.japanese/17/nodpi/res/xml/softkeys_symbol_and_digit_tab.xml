<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_non_prime_symbol_tab" layout="@layout/softkey_icon_without_bar" multi_touch="true" content_description="@string/symbol_keyboard_key_content_desc" alpha="@attr/IconAlpha">
            <action type="PRESS" keycode="OPEN_EXTENSION" data="com.google.android.apps.inputmethod.japanese.symbol.ISymbolKeyboardExtension" />
            <icon location="@id/icon" value="@attr/IconMainCategorySymbol" />
        </softkey>
        <softkey id="@id/softkey_non_prime_symbol_tab_highlighted" layout="@layout/softkey_icon_with_bar" multi_touch="true" content_description="@string/symbol_keyboard_key_content_desc">
            <icon location="@id/icon" value="@attr/IconMainCategorySymbol" />
        </softkey>
        <softkey id="@id/softkey_non_prime_digit_tab" layout="@layout/softkey_icon_without_bar" multi_touch="true" content_description="@string/digit_keyboard_key_content_desc" alpha="@attr/IconAlpha">
            <action type="PRESS" keycode="OPEN_EXTENSION" data="com.google.android.apps.inputmethod.japanese.symbol.IDigitKeyboardExtension" />
            <icon location="@id/icon" value="@attr/IconMainCategoryNumber" />
        </softkey>
        <softkey id="@id/softkey_non_prime_digit_tab_highlighted" layout="@layout/softkey_icon_with_bar" multi_touch="true" content_description="@string/digit_keyboard_key_content_desc">
            <icon location="@id/icon" value="@attr/IconMainCategoryNumber" />
        </softkey>
        <softkey id="@id/softkey_non_prime_emoji_tab_in_extension" layout="@layout/softkey_icon_without_bar" content_description="@string/emoji_keyboard_key_content_desc" alpha="@attr/IconAlpha">
