<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_switch_to_symbols" layout="@layout/softkey_function_key_label_deactive" content_description="@string/symbol_keyboard_key_content_desc">
            <action type="ON_FOCUS" keycode="SWITCH_KEYBOARD" data="symbol" />
            <label location="@id/label" value="\@?#" />
        </softkey>
        <softkey id="@id/softkey_switch_to_prime" layout="@layout/softkey_function_key_label_smalltext_deactive" content_description="@string/letter_keyboard_key_content_desc">
            <action type="ON_FOCUS" keycode="SWITCH_KEYBOARD" data="prime" />
            <label location="@id/label" value="abc123" />
        </softkey>
        <softkey id="@id/softkey_prime_active" layout="@layout/softkey_function_key_label_smalltext" content_description="@string/letter_keyboard_key_content_desc">
            <label location="@id/label" value="abc123" />
        </softkey>
    </softkeys>
</framework>