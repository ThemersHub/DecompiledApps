<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.AppCompat.Tooltip" parent="@style/TextAppearance.AppCompat">
        <item name="android:textSize">14.0sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextAppearance.GoogleMaterial.BottomNavigationView" parent="@style/Base.TextAppearance.GoogleMaterial.BottomNavigationView">
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/google_sans_medium</item>
    </style>
    <style name="TextAppearance.GoogleMaterial.BottomNavigationView.Active" parent="@style/Base.TextAppearance.GoogleMaterial.BottomNavigationView.Active">
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/google_sans_medium</item>
    </style>
    <style name="TextAppearance.GoogleMaterial.Chip.Input" parent="@style/Base.TextAppearance.GoogleMaterial.Chip.Input">
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">sans-serif</item>
    </style>
    <style name="TextAppearance.GoogleMaterial.Chip.Suggestive" parent="@style/Base.TextAppearance.GoogleMaterial.Chip.Suggestive">
        <item name="android:textStyle">normal</item>
        <item name="fontFamily">@font/google_sans_medium</item>
    </style>
    <style name="quantum_text_black_dark">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/quantum_black_text</item>
    </style>
    <style name="quantum_text_black_medium">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/quantum_black_secondary_text</item>
    </style>
    <style name="quantum_text_body_1_black" parent="@style/quantum_text_black_dark">
        <item name="android:textSize">14.0sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="quantum_text_body_2_black" parent="@style/quantum_text_black_dark">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="quantum_text_button_black" parent="@style/quantum_text_black_dark">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="quantum_text_button_white" parent="@style/quantum_text_white_light">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="quantum_text_caption_black" parent="@style/quantum_text_black_medium">
        <item name="android:textSize">12.0sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="quantum_text_menu_black" parent="@style/quantum_text_black_dark">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="quantum_text_subhead_black" parent="@style/quantum_text_black_dark">
        <item name="android:textSize">16.0sp</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="quantum_text_white_light">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/quantum_white_text</item>
    </style>
    <style name="sendkit_ui_text_initial_sheet_contact_method">
        <item name="android:textSize">@dimen/sendkit_ui_face_row_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>
    <style name="sendkit_ui_text_initial_sheet_name">
        <item name="android:textSize">@dimen/sendkit_ui_face_row_text_size</item>
        <item name="android:textStyle">normal</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>
