<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/gmm_white" />
            <corners android:radius="@dimen/qu_flat_button_corner_radius" />
            <stroke android:width="1.0dip" android:color="@color/quantum_googblue" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/qu_flat_button_corner_radius" />
            <stroke android:width="2.0dip" android:color="@color/quantum_googblue700" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/gmm_white" />
            <corners android:radius="@dimen/qu_flat_button_corner_radius" />
            <stroke android:width="1.0dip" android:color="@color/quantum_grey300" />
        </shape>
    </item>
</selector>