<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_activated="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/wallet_uic_chip_button_pressed" />
            <corners android:radius="@dimen/wallet_uic_chip_button_corner_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/wallet_uic_chip_button_normal" />
            <corners android:radius="@dimen/wallet_uic_chip_button_corner_radius" />
        </shape>
    </item>
</selector>