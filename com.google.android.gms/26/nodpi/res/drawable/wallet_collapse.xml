<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <nine-patch android:src="@drawable/wallet_collapse_alpha" android:tint="?android:colorControlActivated" />
    </item>
    <item android:state_focused="true">
        <nine-patch android:src="@drawable/wallet_collapse_alpha" android:tint="?android:colorControlActivated" />
    </item>
    <item>
        <nine-patch android:src="@drawable/wallet_collapse_alpha" android:tint="?android:colorControlNormal" />
    </item>
</selector>