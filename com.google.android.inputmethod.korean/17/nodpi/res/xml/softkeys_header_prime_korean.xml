<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_en_active" layout="@layout/softkey_header_tab_active_no_arrow" content_description="@string/english_dashboard_key_content_desc">
            <icon location="@id/icon" value="@attr/IconHeaderTabEnActive" />
        </softkey>
        <softkey id="@id/softkey_en_active_in_dashboard" layout="@layout/softkey_header_tab_active_no_arrow" content_description="@string/english_keyboard_key_content_desc">
            <action type="PRESS" keycode="SWITCH_TO_PREVIOUS_INPUT_BUNDLE" />
            <icon location="@id/icon" value="@attr/IconHeaderTabEnActive" />
        </softkey>
        <softkey id="@id/softkey_en_deactive" layout="@layout/softkey_header_tab_with_separator" content_description="@string/english_keyboard_key_content_desc">
            <action type="PRESS" keycode="SWITCH_LANGUAGE" data="en" />
            <icon location="@id/icon" value="@attr/IconHeaderTabEnDeactive" />
        </softkey>
        <softkey id="@id/softkey_en_deactive_in_dashboard" layout="@layout/softkey_header_tab_with_separator" content_description="@string/english_dashboard_key_content_desc">
            <action type="PRESS" keycode="SWITCH_LANGUAGE" data="en" />
            <icon location="@id/icon" value="@attr/IconHeaderTabEnDeactive" />
        </softkey>
        <softkey id="@id/softkey_korean_deactive" layout="@layout/softkey_icon" content_description="@string/korean_keyboard_key_content_desc">
            <action type="PRESS" keycode="SWITCH_LANGUAGE" data="@string/korean_language_id" />
            <icon location="@id/icon" value="@attr/IconHeaderTabKoreanDeactive" />
        </softkey>
        <softkey id="@id/softkey_korean_deactive_in_dashboard" layout="@layout/softkey_icon" content_description="@string/korean_dashboard_key_content_desc">
            <action type="PRESS" keycode="SWITCH_LANGUAGE" data="@string/korean_language_id" />
            <icon location="@id/icon" value="@attr/IconHeaderTabKoreanDeactive" />
        </softkey>
        <softkey id="@id/softkey_korean_active" layout="@layout/softkey_header_tab_active_no_arrow" content_description="@string/korean_dashboard_key_content_desc">
            <icon location="@id/icon" value="@attr/IconHeaderTabKoreanActive" />
        </softkey>
        <softkey id="@id/softkey_korean_active_in_dashboard" layout="@layout/softkey_header_tab_active" content_description="@string/korean_keyboard_key_content_desc">
            <action type="PRESS" keycode="SWITCH_TO_PREVIOUS_INPUT_BUNDLE" />
            <icon location="@id/icon" value="@attr/IconHeaderTabKoreanActive" />
        </softkey>
    </softkeys>
</framework>