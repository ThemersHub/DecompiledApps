<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="HotwordEnrollmentButton" parent="@style/HotwordEnrollmentButtonBase">
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="HotwordEnrollmentButtonBar" parent="@style/HotwordEnrollmentButtonBarBase">
        <item name="android:gravity">end</item>
        <item name="android:paddingLeft">@dimen/hotword_enrollment_content_padding</item>
        <item name="android:paddingRight">@dimen/hotword_enrollment_content_padding</item>
    </style>
    <style name="HotwordEnrollmentIntroImage">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">380.0dip</item>
        <item name="android:src">@drawable/hotword_header_tablet_image</item>
    </style>
    <style name="HotwordEnrollmentIntroImageGroup">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">170.0dip</item>
    </style>
    <style name="HotwordEnrollmentIntroLogo">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">50.0dip</item>
    </style>
    <style name="NamedSequence.Default" parent="@style/Opa.Activity.BaseTheme">
        <item name="buttonFooterLayout">@layout/footer_cancel_and_action_button</item>
        <item name="suwGlifHeaderGravity">left</item>
    </style>
    <style name="Opa.Activity.Theme" parent="@style/Opa.Activity.BaseTheme">
        <item name="suwGlifHeaderGravity">start</item>
    </style>
    <style name="OpaErrorContentContainer">
        <item name="android:paddingLeft">@dimen/opa_error_margin</item>
        <item name="android:paddingTop">@dimen/opa_error_margin_top</item>
        <item name="android:paddingRight">@dimen/opa_error_margin</item>
    </style>
    <style name="OpaValuePropContentContainer">
        <item name="android:gravity">start</item>
    </style>
    <style name="OpaValuePropContentMessage" parent="@style/Opa.Text.Message">
        <item name="android:gravity">start</item>
    </style>
    <style name="OpaValuePropContentTitle" parent="@style/Opa.Text.Title">
        <item name="android:gravity">start</item>
    </style>
    <style name="SnackbarContainer">
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="Theme.HotwordEnrollment.Glif" parent="@style/Theme.HotwordEnrollment.BaseGlif">
        <item name="suwGlifHeaderGravity">start</item>
    </style>
    <style name="Widget.Design.TabLayout" parent="@style/Base.Widget.Design.TabLayout">
        <item name="tabMode">fixed</item>
        <item name="tabGravity">center</item>
    </style>
</resources>
