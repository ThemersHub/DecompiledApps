<?xml version="1.0" encoding="utf-8"?>
<transitionSet android:transitionOrdering="sequential"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <transitionSet android:transitionOrdering="together">
        <transition android:duration="200" app:fadingMode="fade_out" class="com.google.android.apps.gsa.staticplugins.quartz.monet.shared.transition.FadeTextTransition" />
        <transition android:duration="200" app:fadingMode="fade_out" class="com.google.android.apps.gsa.staticplugins.quartz.monet.shared.transition.FadeTranslationTransition" />
        <fade android:duration="200" android:fadingMode="fade_out" />
    </transitionSet>
    <changeBounds android:duration="0" />
    <transitionSet android:transitionOrdering="together">
        <transition android:duration="300" app:fadingMode="fade_in" class="com.google.android.apps.gsa.staticplugins.quartz.monet.shared.transition.FadeTextTransition" />
        <transition android:duration="300" app:fadingMode="fade_in" class="com.google.android.apps.gsa.staticplugins.quartz.monet.shared.transition.FadeTranslationTransition" />
        <fade android:duration="300" android:fadingMode="fade_in" />
    </transitionSet>
</transitionSet>