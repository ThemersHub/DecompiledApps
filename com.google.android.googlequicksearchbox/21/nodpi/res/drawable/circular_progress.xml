<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:useLevel="true" android:thickness="4.0dip">
            <gradient android:startColor="@color/quartz_white_30p" android:endColor="@color/quartz_white_30p" android:type="sweep" android:centerColor="@color/quartz_white_30p" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:useLevel="true" android:thickness="4.0dip">
                <rotate android:fromDegrees="0.0" android:toDegrees="360.0" android:pivotX="50.0%" android:pivotY="50.0%" />
                <gradient android:startColor="@color/quantum_white_100" android:endColor="@color/quantum_white_100" android:type="sweep" android:centerColor="@color/quantum_white_100" />
            </shape>
        </rotate>
    </item>
</layer-list>