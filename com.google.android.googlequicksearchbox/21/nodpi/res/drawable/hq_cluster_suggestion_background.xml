<?xml version="1.0" encoding="utf-8"?>
<selector android:visible="true" android:enterFadeDuration="@integer/ripple_or_fade_duration_ms"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="#8a000000" />
            <solid android:color="@color/hq_suggestion_background_pressed" />
            <corners android:radius="@dimen/hq_action_chip_radius" />
            <padding android:left="@dimen/hq_action_chip_side_padding" android:top="@dimen/hq_action_chip_top_padding" android:right="@dimen/hq_action_chip_side_padding" android:bottom="@dimen/hq_action_chip_bottom_padding" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="#57000000" />
            <corners android:radius="@dimen/hq_action_chip_radius" />
            <padding android:left="@dimen/hq_action_chip_side_padding" android:top="@dimen/hq_action_chip_top_padding" android:right="@dimen/hq_action_chip_side_padding" android:bottom="@dimen/hq_action_chip_bottom_padding" />
        </shape>
    </item>
</selector>