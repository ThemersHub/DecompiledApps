<?xml version="1.0" encoding="utf-8"?>
<selector android:visible="true" android:enterFadeDuration="@integer/ripple_or_fade_duration_ms"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/hq_action_chip_stroke_width" android:color="@color/hq_suggestion_outline" />
            <corners android:radius="@dimen/hq_action_chip_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/hq_action_chip_stroke_width" android:color="@color/hq_suggestion_outline" />
            <corners android:radius="@dimen/hq_action_chip_radius" />
        </shape>
    </item>
</selector>