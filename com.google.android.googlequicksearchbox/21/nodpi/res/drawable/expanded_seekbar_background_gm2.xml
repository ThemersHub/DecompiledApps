        <scale android:drawable="@color/google_grey200" android:scaleWidth="100%" />
    </item>
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="rectangle">
            <solid android:color="#404284f4" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <scale android:drawable="@color/google_blue600" android:scaleWidth="100%" />
    </item>
</layer-list>