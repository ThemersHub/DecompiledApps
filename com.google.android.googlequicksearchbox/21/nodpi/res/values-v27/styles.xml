<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomSheetDialog" parent="@style/Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/white</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">@color/back_of_card_navigation_line_color</item>
    </style>
    <style name="Opa.Activity.Theme" parent="@style/Opa.Activity.BaseTheme">
        <item name="android:navigationBarColor">@color/light_navigation_bar_color</item>
        <item name="suwGlifHeaderGravity">center_horizontal</item>
    </style>
    <style name="OpaHQActivity.Theme" parent="@style/OpaHQActivity.Theme.Base">
        <item name="android:navigationBarColor">@color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="SuwThemeGlifV3.Light" parent="@style/SuwBaseThemeGlifV3.Light">
        <item name="android:navigationBarColor">@color/suw_glif_v3_nav_bar_color_light</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">@color/suw_glif_v3_nav_bar_divider_color_light</item>
    </style>
    <style name="Theme.AssistantSettings" parent="@style/Theme.AssistantSettings.Base">
        <item name="android:navigationBarColor">@color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="Theme.AssistantSettings.NoActionBar" parent="@style/Theme.AssistantSettings">
        <item name="android:navigationBarColor">@color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.DynamicHost" parent="@style/Theme.Shim.Light">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:navigationBarColor">#ffffffff</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">#ffe0e0e0</item>
    </style>
    <style name="Theme.DynamicHost.Translucent" parent="@style/Theme.DynamicHost">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.GoogleMaterial.Light" parent="@style/Base.Theme.GoogleMaterial.Light">
        <item name="android:navigationBarColor">@color/google_white</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>
    <style name="Theme.GoogleMaterial.Light.BottomSheetDialog" parent="@style/Base.Theme.GoogleMaterial.Light.BottomSheetDialog">
        <item name="android:navigationBarColor">@color/google_white</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">@android:color/transparent</item>
    </style>
    <style name="Theme.Gsa" parent="@style/Theme.Shim.Light">
        <item name="android:textColorHighlight">@color/qp_highlight</item>
        <item name="android:actionBarStyle">@style/Gsa</item>
        <item name="android:alertDialogTheme">@style/Theme.Gsa.Dialog</item>
        <item name="android:textColorHighlightInverse">@color/qp_highlight</item>
        <item name="android:colorControlActivated">@color/qp_blue</item>
        <item name="android:colorPrimary">@color/qp_blue</item>
        <item name="android:colorPrimaryDark">@android:color/black</item>
        <item name="android:colorAccent">@color/qp_blue</item>
        <item name="android:navigationBarColor">@color/light_navigation_bar_color</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">@color/light_navigation_bar_divider_color</item>
        <item name="qpActionIconAlpha">0.38</item>
        <item name="qpActionIconTint">@android:color/black</item>
        <item name="qpH1FontFamily">sans-serif-light</item>
        <item name="qpH1TextSize">@dimen/qp_h1_text_size</item>
        <item name="vogPromptColor">@color/qp_text_b2</item>
        <item name="vogPromptTextStyle">2</item>
    </style>
    <style name="Theme.Transparent" parent="@style/Theme.Velvet">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
    <style name="Theme.Velvet.BlackStatusBarAndNavBar" parent="@style/Theme.Velvet">
        <item name="android:statusBarColor">@android:color/black</item>
        <item name="android:navigationBarColor">@android:color/black</item>
        <item name="android:windowLightNavigationBar">false</item>
        <item name="android:navigationBarDividerColor">@android:color/black</item>
    </style>
    <style name="TransparentMonetActivity">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowSoftInputMode">adjustNothing|stateUnchanged</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">#ffffffff</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:navigationBarDividerColor">#ffe0e0e0</item>
    </style>
</resources>
