<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <com.google.android.apps.gsa.search.core.preferences.SelectAccountPreference android:persistent="false" android:title="@string/select_google_account" android:key="google_account" android:summary="@string/select_google_account_summary_no_accounts" />
    <Preference android:persistent="false" android:title="@string/relationships" android:key="relationships" android:summary="@string/relationships_summary" android:fragment="com.google.android.apps.gsa.search.core.preferences.cards.RelationshipSettingsFragment" />
    <Preference android:persistent="false" android:title="@string/google_account_search_history" android:key="google_account_search_history" android:summary="@string/google_account_search_history_summary" />
    <Preference android:persistent="false" android:title="@string/google_account_history" android:key="google_account_history" android:summary="@string/google_account_history_summary" />
    <SwitchPreference android:persistent="true" android:title="@string/use_recently" android:key="use_recently" android:defaultValue="true" android:summaryOn="@string/use_recently_subtitle_when_enabled" android:summaryOff="@string/use_recently_subtitle_when_disabled" />
    <Preference android:persistent="false" android:title="@string/signed_out_search_title" android:key="signed_out_search" android:fragment="com.google.android.apps.gsa.search.core.preferences.SignedOutPersonalizationFragment" />
    <SwitchPreference android:persistent="true" android:title="@string/safe_search_title" android:key="safe_search" android:defaultValue="false" android:summaryOn="@string/safe_search_summary_on" android:summaryOff="@string/safe_search_summary_off" />
    <SwitchPreference android:persistent="true" android:title="@string/use_custom_tabs" android:key="use_custom_tabs" android:defaultValue="true" android:summaryOn="@string/use_custom_tabs_enabled_summary" android:summaryOff="@string/use_custom_tabs_disabled_summary" />
    <com.google.android.apps.gsa.search.core.preferences.GelUsageStatsSwitchPreference android:persistent="true" android:title="@string/gel_usage_stats_title" android:key="gel_usage_stats" android:summary="@string/gel_usage_stats_summary" android:defaultValue="false" />
</PreferenceScreen>