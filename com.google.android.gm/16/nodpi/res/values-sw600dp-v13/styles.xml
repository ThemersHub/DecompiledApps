<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AccountsSwitcher.AccountTextStyle" parent="@style/AccountsSwitcher">
        <item name="android:textColor">@color/add_accounts_text_color</item>
        <item name="android:layout_marginLeft">@dimen/start_horiz_padding_tablet</item>
    </style>
    <style name="AccountsSwitcher.AvatarStyle.Gmail" parent="@style/AccountsSwitcher.AvatarStyle">
        <item name="android:layout_marginLeft">@dimen/start_horiz_padding_tablet</item>
    </style>
    <style name="AccountsSwitcher.OwnersListItemStyle" parent="@style/AccountsSwitcher">
        <item name="android:paddingLeft">@dimen/start_horiz_padding_tablet</item>
        <item name="android:paddingRight">@dimen/start_horiz_padding</item>
    </style>
    <style name="ComposeAreaWrapper">
        <item name="android:background">@color/compose_background</item>
    </style>
    <style name="ComposeScrollView" />
    <style name="FolderListItemPaddingStyle">
        <item name="android:paddingLeft">@dimen/folder_list_item_padding_side_tablet</item>
        <item name="android:paddingRight">@dimen/folder_list_item_padding_side</item>
    </style>
    <style name="GmailifyHeader">
        <item name="suwBackgroundTile">@color/quantum_googblue700</item>
        <item name="suwIllustrationHorizontalTile">@drawable/gmailify_header_repeat_tablet</item>
        <item name="suwIllustrationImage">@drawable/gmailify_header_main_tablet</item>
    </style>
    <style name="HatsSurveyHolder">
        <item name="android:layout_gravity">start|bottom|center</item>
        <item name="android:layout_width">@dimen/hats_holder_width_tablet</item>
    </style>
    <style name="RecipientEditTextView" parent="@style/RecipientEditTextViewBase">
        <item name="android:imeOptions">actionNext</item>
    </style>
    <style name="RichTextToolbar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/compose_rich_text_toolbar_margin_end</item>
        <item name="android:layout_marginBottom">@dimen/compose_rich_text_toolbar_margin_bottom</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="SetupWizardHeaderStyle">
        <item name="suwBackgroundTile">@drawable/ic_suw_bg_tile</item>
        <item name="suwIllustrationHorizontalTile">@drawable/ic_suw_bg_header_tile</item>
        <item name="suwIllustrationImage">@drawable/ic_suw_tablet_header_01</item>
    </style>
    <style name="SnackBar.Text.Action.SingleLine" parent="@style/SnackBar.Text.Action">
        <item name="android:maxWidth">190.0dip</item>
    </style>
    <style name="VacationResponderTheme" parent="@style/UnifiedEmailTheme.Appcompat">
        <item name="android:windowBackground">@color/compose_background</item>
    </style>
    <style name="WelcomeTour.Container" parent="@style/WelcomeTour">
        <item name="android:layout_gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">@dimen/welcome_tour_container_width</item>
        <item name="android:layout_height">@dimen/welcome_tour_container_height</item>
    </style>
    <style name="WelcomeTour.ContentArea" parent="@style/WelcomeTour">
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">@dimen/welcome_tour_content_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/welcome_tour_content_padding_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="WelcomeTour.ImageArea" parent="@style/WelcomeTour">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_marginTop">@dimen/welcome_tour_image_margin_top</item>
        <item name="android:layout_weight">1.0</item>
    </style>
</resources>
