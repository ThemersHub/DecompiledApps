<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="@dimen/flat_blue_button_with_white_background_border_radius" />
            <stroke android:width="@dimen/flat_blue_button_with_white_background_border_width" android:color="@color/quantum_googblue500" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/flat_blue_button_with_white_background_border_radius" />
            <stroke android:width="@dimen/flat_blue_button_with_white_background_border_width" android:color="@color/quantum_googblue500" />
            <solid android:color="@color/quantum_googblue300" />
        </shape>
    </item>
</selector>