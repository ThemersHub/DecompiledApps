<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_chechen" class="com.google.android.apps.inputmethod.libs.latin5.LatinIme" language="ce-RU" auto_capital="true" display_app_completions="true" announce_auto_selected_candidate="true">
        <if number_row="false">
            <keyboard_group variant="chechen" variant_label="@string/variant_chechen">
                <keyboard type="prime" def="@xml/keyboard_ce_ru" />
                <include href="@xml/keyboards_non_prime_latin" />
            </keyboard_group>
        </if>
        <else>
            <keyboard_group variant="chechen" variant_label="@string/variant_chechen" layout_theme="@style/KeyboardLayoutTheme.WithNumberRow">
                <keyboard type="prime" def="@xml/keyboard_ce_ru">
                    <merge def="@xml/keyboard_latin_fragment_numbers_10_12_12_11" />
                </keyboard>
                <include href="@xml/keyboards_non_prime_latin" />
            </keyboard_group>
        </else>
    </ime>
</framework>