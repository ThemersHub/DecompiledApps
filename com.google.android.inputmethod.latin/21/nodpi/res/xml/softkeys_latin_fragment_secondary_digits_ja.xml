<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@+id/softkey_template_merge_header" layout="@layout/softkey_qwerty_label_character_label_header" popup_timing="@string/qwerty_popup_timing" slide_sensitivity="NORMAL" multi_touch="false" content_description="$desc$">
            <action type="LONG_PRESS" data="$long_press_data$" keycode="PLAIN_TEXT" intention="$long_press_intention=DECODE$" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" />
            <action type="SLIDE_UP" data="$long_press_data$" keycode="PLAIN_TEXT" intention="$long_press_intention=DECODE$" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" />
            <label location="@id/label_header" value="$long_press_data$" />
        </softkey_template>
        <softkey_list splitter="," template_id="@id/softkey_template_merge_header">
            <softkey id="@id/softkey_fragment_superscript_digit_one" long_press_data="1" />
            <softkey id="@id/softkey_fragment_superscript_digit_two" long_press_data="2" />
            <softkey id="@id/softkey_fragment_superscript_digit_three" long_press_data="3" />
            <softkey id="@id/softkey_fragment_superscript_digit_four" long_press_data="4" />
            <softkey id="@id/softkey_fragment_superscript_digit_five" long_press_data="5" />
            <softkey id="@id/softkey_fragment_superscript_digit_six" long_press_data="6" />
            <softkey id="@id/softkey_fragment_superscript_digit_seven" long_press_data="7" />
            <softkey id="@id/softkey_fragment_superscript_digit_eight" long_press_data="8" />
            <softkey id="@id/softkey_fragment_superscript_digit_nine" long_press_data="9" />
            <softkey id="@id/softkey_fragment_superscript_digit_zero" long_press_data="0" />
        </softkey_list>
    </softkeys>
</framework>