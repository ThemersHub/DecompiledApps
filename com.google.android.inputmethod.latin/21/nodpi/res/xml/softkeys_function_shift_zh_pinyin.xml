<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey id="@id/softkey_chinese_semicolon_in_shuangpin_darkkey" layout="@layout/softkey_label_medium" multi_touch="false">
            <action type="DOWN" keycode="SHIFT_DOWN" />
            <action type="UP" keycode="SHIFT_UP" />
            <action type="PRESS" keycode="SEMICOLON" data=";" intention="DECODE" />
            <label location="@id/label" value=";" />
        </softkey>
        <softkey id="@id/softkey_ci" layout="@layout/softkey_icon" multi_touch="false" content_description="@string/separator_key_content_desc" alpha="@attr/LabelAlpha">
            <action type="DOWN" keycode="SHIFT_DOWN" />
            <action type="UP" keycode="SHIFT_UP" />
            <action type="PRESS" keycode="APOSTROPHE" data="'" intention="DECODE" />
            <if language="zh-HK|zh-TW|zh-XG">
                <icon location="@id/icon" value="@attr/IconCiTraditional" />
            </if>
            <else>
                <icon location="@id/icon" value="@attr/IconCiSimplified" />
            </else>
        </softkey>
        <softkey id="@id/softkey_chinese_semicolon_in_shuangpin" layout="@layout/softkey_label_medium_auto_centered" multi_touch="false">
            <action type="PRESS" keycode="SEMICOLON" data=";" intention="DECODE" />
            <label location="@id/label" value=";" />
        </softkey>
    </softkeys>
</framework>