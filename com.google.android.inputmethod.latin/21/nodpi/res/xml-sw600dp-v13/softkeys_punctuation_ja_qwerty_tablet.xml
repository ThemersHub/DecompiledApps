<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_list splitter=" ">
            <softkey_template id="@+id/softkey_template_punctuation_ja_qwerty" layout="@layout/softkey_qwerty_label_character" popup_timing="@string/qwerty_popup_timing" slide_sensitivity="NORMAL" multi_touch="false" content_description="$desc$">
                <action type="PRESS" data="$press_data$" keycode="$keycode_normal$" intention="DECODE" popup_label="$label$" />
                <label location="@id/label" value="$label$" />
            </softkey_template>
        </softkey_list>
        <softkey_list template_id="@id/softkey_template_punctuation_ja_qwerty" splitter=" ">
            <softkey id="@id/softkey_ja_qwerty_exclamation_tablet" label="!" press_data="!" keycode_normal="PLAIN_TEXT" />
            <softkey id="@id/softkey_ja_qwerty_question_tablet" label="\?" press_data="\?" keycode_normal="PLAIN_TEXT" />
        </softkey_list>
    </softkeys>
</framework>