<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/dataviz_background" />
            <corners android:radius="@dimen/dataviz_layout_rounded_border_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/item_selection" />
            <corners android:radius="@dimen/dataviz_layout_rounded_border_radius" />
        </shape>
    </item>
</selector>