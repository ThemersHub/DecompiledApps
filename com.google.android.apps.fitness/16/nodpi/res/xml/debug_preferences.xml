<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference android:title="@string/preferences_sessions_debug_ui_title" android:key="debug_ui" android:defaultValue="false" />
    <CheckBoxPreference android:title="@string/preferences_show_memory_use_title" android:key="show_memory_use" android:defaultValue="true" />
    <CheckBoxPreference android:title="@string/preferences_wheel_view_debug" android:key="wheel_view_debug" />
    <CheckBoxPreference android:title="@string/preferences_show_chart_debug" android:key="show_chart_cache" android:defaultValue="false" />
    <CheckBoxPreference android:title="@string/preferences_show_loader_toasts" android:key="show_loader_toasts" android:defaultValue="false" />
    <CheckBoxPreference android:title="@string/preferences_use_maps_elevation" android:key="maps_elevation" android:defaultValue="true" />
    <CheckBoxPreference android:title="@string/preferences_show_map_paths" android:key="maps_paths" android:defaultValue="false" />
    <EditTextPreference android:title="@string/preferences_force_generate_notifications" android:key="force_generate_notifications" />
    <CheckBoxPreference android:title="Show all myfit cards" android:key="show_all_myfit_cards" android:defaultValue="false" />
