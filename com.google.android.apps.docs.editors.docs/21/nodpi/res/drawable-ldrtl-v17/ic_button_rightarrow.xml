<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <rotate android:drawable="@drawable/ic_button_rightarrow_press_24" android:fromDegrees="180.0" android:toDegrees="180.0" android:pivotX="50.0%" android:pivotY="50.0%"
          xmlns:android="http://schemas.android.com/apk/res/android" />
    </item>
    <item android:state_focused="true">
        <rotate android:drawable="@drawable/ic_button_rightarrow_press_24" android:fromDegrees="180.0" android:toDegrees="180.0" android:pivotX="50.0%" android:pivotY="50.0%"
          xmlns:android="http://schemas.android.com/apk/res/android" />
    </item>
    <item>
        <rotate android:drawable="@drawable/ic_button_rightarrow_normal_24" android:fromDegrees="180.0" android:toDegrees="180.0" android:pivotX="50.0%" android:pivotY="50.0%"
          xmlns:android="http://schemas.android.com/apk/res/android" />
    </item>
</selector>