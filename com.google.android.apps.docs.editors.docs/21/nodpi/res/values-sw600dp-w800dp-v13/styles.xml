<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="insert_tool_actionbar_icon">
        <item name="android:gravity">center|left</item>
        <item name="android:background">@drawable/uxf_uncheckable_button_background_white_icon</item>
        <item name="android:paddingLeft">@dimen/insert_tool_actionbar_icon_padding_left_right</item>
        <item name="android:paddingRight">@dimen/insert_tool_actionbar_icon_padding_left_right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:paddingStart">@dimen/insert_tool_actionbar_icon_padding_left_right</item>
        <item name="android:paddingEnd">@dimen/insert_tool_actionbar_icon_padding_left_right</item>
    </style>
    <style name="insert_tool_actionbar_query">
        <item name="android:theme">@android:style/Theme.Material</item>
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:lines">1</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:paddingStart">@dimen/insert_tool_actionbar_text_margin_left_right</item>
        <item name="android:paddingEnd">@dimen/insert_tool_actionbar_text_margin_left_right</item>
    </style>
    <style name="insert_tool_actionbar_zero_search_title" parent="@style/insert_tool_actionbar_query">
        <item name="android:textSize">@dimen/insert_tool_actionbar_zero_search_text_size</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="insert_tool_card_title">
        <item name="android:textSize">@dimen/insert_tool_card_header_text_size</item>
        <item name="android:textColor">@color/insert_tool_card_title</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/insert_tool_card_padding_left_right</item>
        <item name="android:paddingTop">@dimen/insert_tool_card_title_padding</item>
        <item name="android:paddingRight">@dimen/insert_tool_card_padding_left_right</item>
        <item name="android:paddingBottom">@dimen/insert_tool_card_title_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:paddingStart">@dimen/insert_tool_card_padding_left_right</item>
        <item name="android:paddingEnd">@dimen/insert_tool_card_padding_left_right</item>
    </style>
    <style name="insert_tool_document_search_row_text">
        <item name="android:orientation">vertical</item>
    </style>
    <style name="insert_tool_document_title_text">
        <item name="android:layout_width">fill_parent</item>
    </style>
    <style name="insert_tool_edit_link_clear_button">
        <item name="android:background">?android:selectableItemBackgroundBorderless</item>
    </style>
    <style name="insert_tool_edit_link_text_field" parent="@style/floating_label_edit_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:paddingLeft">0.0dip</item>
        <item name="android:paddingTop">18.0dip</item>
        <item name="android:paddingRight">32.0dip</item>
        <item name="android:paddingBottom">12.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">16.0dip</item>
        <item name="android:paddingStart">0.0dip</item>
        <item name="android:paddingEnd">32.0dip</item>
    </style>
    <style name="insert_tool_heading">
        <item name="android:orientation">horizontal</item>
        <item name="android:id">@id/heading</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">48.0dip</item>
    </style>
    <style name="insert_tool_heading_shadow">
        <item name="android:background">@drawable/insert_tool_overlap_shadow</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/editor_overlap_shadow_height</item>
    </style>
    <style name="insert_tool_heading_text_button" parent="@android:style/Widget.Material.Button.Borderless.Small">
        <item name="android:theme">@android:style/Theme.Material</item>
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/inserttool_insert_button_text_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">@dimen/insert_tool_actionbar_icon_padding_left_right</item>
        <item name="android:paddingEnd">@dimen/insert_tool_actionbar_icon_padding_left_right</item>
    </style>
    <style name="insert_tool_label">
        <item name="android:textSize">@dimen/insert_tool_normal_text_size</item>
        <item name="android:textColor">@color/insert_tool_label_text</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="insert_tool_more_button" parent="@style/Base.Widget.AppCompat.Button.Borderless">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textSize">@dimen/insert_tool_card_header_text_size</item>
        <item name="android:textColor">@color/insert_tool_actionbar</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">48.0dip</item>
        <item name="android:text">@string/insert_tool_more_button</item>
        <item name="android:paddingStart">@dimen/insert_tool_more_button_padding</item>
        <item name="android:paddingEnd">@dimen/insert_tool_more_button_padding</item>
    </style>
    <style name="insert_tool_progress_bar" parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:gravity">top</item>
        <item name="android:id">@id/insert_tool_progress_bar</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">-6.0dip</item>
        <item name="android:layout_marginBottom">0.0dip</item>
    </style>
    <style name="insert_tool_search_actionbar_icon" parent="@style/insert_tool_actionbar_icon">
        <item name="android:background">@drawable/uxf_uncheckable_button_background</item>
    </style>
    <style name="insert_tool_search_bar" parent="@android:style/Widget.EditText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="insert_tool_title">
        <item name="android:textSize">@dimen/insert_tool_title_text_size</item>
        <item name="android:textColor">@color/insert_tool_normal_text</item>
        <item name="android:fontFamily">sans-serif-light</item>
    </style>
    <style name="insert_tool_touch_focusable_heading" parent="@style/insert_tool_heading">
        <item name="android:background">@drawable/insert_tool_fake_search_bar_background</item>
        <item name="android:focusableInTouchMode">true</item>
    </style>
    <style name="insert_tool_zero_search_card">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/insert_tool_card_margin_left_right</item>
        <item name="android:layout_marginTop">@dimen/insert_tool_card_margin_top</item>
        <item name="android:layout_marginRight">@dimen/insert_tool_card_margin_left_right</item>
        <item name="android:layout_marginBottom">@dimen/insert_tool_card_margin_bottom</item>
        <item name="android:layout_marginStart">@dimen/insert_tool_card_margin_left_right</item>
        <item name="android:layout_marginEnd">@dimen/insert_tool_card_margin_left_right</item>
    </style>
</resources>
