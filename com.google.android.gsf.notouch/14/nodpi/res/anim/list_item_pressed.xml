<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <set>
            <objectAnimator android:duration="@integer/list_item_pressed_fade_duration" android:valueTo="0.2" android:valueType="floatType" android:propertyName="alpha" />
        </set>
    </item>
    <item>
        <set>
            <objectAnimator android:duration="0" android:valueTo="1.0" android:valueType="floatType" android:propertyName="alpha" />
        </set>
    </item>
</selector>