<?xml version="1.0" encoding="utf-8"?>
<framework>
    <ime string_id="ime_ukrainian" class="com.google.android.apps.inputmethod.libs.latin5.LatinIme" language="uk" auto_capital="true" display_app_completions="true" announce_auto_selected_candidate="true">
        <if number_row="false">
            <keyboard_group variant="ukrainian" variant_label="@string/variant_ukrainian">
                <keyboard type="prime" def="@xml/keyboard_uk" />
                <include href="@xml/keyboards_non_prime_latin" />
            </keyboard_group>
        </if>
        <else>
            <keyboard_group variant="ukrainian" variant_label="@string/variant_ukrainian" layout_theme="@style/KeyboardLayoutTheme.WithNumberRow">
                <keyboard type="prime" def="@xml/keyboard_uk">
                    <merge def="@xml/keyboard_latin_fragment_numbers_10_12_12_12" />
                </keyboard>
                <include href="@xml/keyboards_non_prime_latin" />
            </keyboard_group>
        </else>
    </ime>
</framework>