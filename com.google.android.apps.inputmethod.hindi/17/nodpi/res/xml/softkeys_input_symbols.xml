        <softkey_template id="@id/softkey_template_symbol_multi_popup" layout="@layout/softkey_label_medium" popup_timing="@string/qwerty_popup_timing" multi_touch="false" content_description="$desc$">
            <action type="PRESS" data="$param_data$" keycode="$keycode_press=PLAIN_TEXT$" intention="COMMIT" popup_label="$param_data$" />
            <action type="LONG_PRESS" data="$long_press_data$" keycode="PLAIN_TEXT" intention="COMMIT" popup_label="$long_press_data$" popup_layout="@attr/PopupBubbleRectangularLayout" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_symbol_multi_popup" splitter=",">
            <softkey id="@id/softkey_symbol_rupee" param_data="₹" long_press_data="$,¢,€,£,¥" desc="@string/rupee_key_content_desc" />
            <softkey id="@id/softkey_symbol_star" param_data="*" keycode_press="STAR" long_press_data="†,★,‡" />
