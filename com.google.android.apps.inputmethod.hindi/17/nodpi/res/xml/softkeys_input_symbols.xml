            <softkey id="@id/softkey_symbol_question" param_data="\?" />
            <softkey id="@id/softkey_symbol_wave" param_data="~" />
            <softkey id="@id/softkey_symbol_stop" param_data="`" />
            <softkey id="@id/softkey_symbol_vertical_bar" param_data="|" />
