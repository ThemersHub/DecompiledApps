            <softkey id="@id/softkey_symbol_left_solid_square_bracket" param_data="【" desc="@string/left_black_square_bracket_key_content_desc" />
            <softkey id="@id/softkey_symbol_right_solid_square_bracket" param_data="】" desc="@string/right_black_square_bracket_key_content_desc" />
