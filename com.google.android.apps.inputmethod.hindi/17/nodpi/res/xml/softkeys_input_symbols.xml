<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@id/softkey_template_symbol_single_normal_popup" layout="@layout/softkey_one_label_en_key" popup_timing="@string/qwerty_popup_timing" multi_touch="false" content_description="$desc$">
            <action type="PRESS" data="$param_data$" keycode="$keycode_press=PLAIN_TEXT$" intention="COMMIT" popup_label="$param_data$" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_symbol_single_normal_popup">
            <softkey id="@id/softkey_symbol_at" param_data="\@" keycode_press="AT" />
            <softkey id="@id/softkey_symbol_pond" param_data="\#" keycode_press="POUND" />
            <softkey id="@id/softkey_symbol_and" param_data="&amp;" />
            <softkey id="@id/softkey_symbol_exclamation" param_data="!" />
            <softkey id="@id/softkey_symbol_colon" param_data=":" />
