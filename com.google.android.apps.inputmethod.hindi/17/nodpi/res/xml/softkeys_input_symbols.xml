            <softkey id="@id/softkey_symbol_back_slash" param_data="\\" keycode_press="BACKSLASH" />
            <softkey id="@id/softkey_symbol_two_comma" param_data="„" />
            <softkey id="@id/softkey_symbol_ellipsis" param_data="…" />
