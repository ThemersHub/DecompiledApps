            <softkey id="@id/softkey_symbol_solid_square" param_data="■" long_press_data="□" />
            <softkey id="@id/softkey_symbol_solid_rhombus" param_data="◆" long_press_data="◇" />
            <softkey id="@id/softkey_symbol_solid_big_circle" param_data="●" long_press_data="○" />
            <softkey id="@id/softkey_symbol_degree" param_data="°" long_press_data="’,”" />
            <softkey id="@id/softkey_symbol_hat" param_data="^" long_press_data="↑,↓,←,→" />
            <softkey id="@id/softkey_symbol_equal" param_data="=" keycode_press="EQUALS" long_press_data="≠,≈,∞" />
            <softkey id="@id/softkey_symbol_less_than" param_data="&lt;" long_press_data="≤,≪,‹" />
