<?xml version="1.0" encoding="utf-8"?>
<framework>
    <softkeys>
        <softkey_template id="@+id/softkey_template_password_symbol" layout="@layout/softkey_one_label_en_key" popup_timing="@string/qwerty_popup_timing" multi_touch="false">
            <action type="PRESS" keycode="$param_keycode=PLAIN_TEXT$" data="$param_data$" intention="COMMIT" popup_label="$param_data$" />
            <label location="@id/label" value="$param_data$" />
        </softkey_template>
        <softkey_list template_id="@id/softkey_template_password_symbol">
            <softkey id="@id/softkey_light_exclamation_mark" param_data="!" />
            <softkey id="@id/softkey_light_at" param_keycode="AT" param_data="\@" />
            <softkey id="@id/softkey_light_pound" param_keycode="POUND" param_data="\#" />
            <softkey id="@id/softkey_light_dollar" param_data="$" />
            <softkey id="@id/softkey_light_star" param_keycode="STAR" param_data="*" />
            <softkey id="@id/softkey_light_percent" param_data="%" />
            <softkey id="@id/softkey_light_hat" param_data="^" />
            <softkey id="@id/softkey_light_and" param_data="&amp;" />
            <softkey id="@id/softkey_light_left_bracket" param_data="(" />
            <softkey id="@id/softkey_light_right_bracket" param_data=")" />
        </softkey_list>
    </softkeys>
</framework>