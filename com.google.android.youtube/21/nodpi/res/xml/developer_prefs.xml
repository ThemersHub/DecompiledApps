        <Preference android:title="@string/pref_developer_offline_browse_response_sync_title" android:key="OfflineBrowseResponseSync" android:summary="@string/pref_developer_offline_browse_response_sync_summary" />
        <Preference android:title="@string/pref_developer_trigger_auto_offline_title" android:key="TriggerAO" android:summary="@string/pref_developer_trigger_auto_offline_summary" />
