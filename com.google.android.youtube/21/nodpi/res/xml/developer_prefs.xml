        </PreferenceScreen>
        <ListPreference android:entries="@array/pref_developer_mdx_orbit_app_version_entries" android:title="@string/pref_developer_mdx_orbit_app_version_title" android:key="MdxReceiverAppVersion" android:summary="@string/pref_developer_mdx_orbit_app_version_summary" android:defaultValue="@string/pref_developer_mdx_orbit_app_version_default_value" android:entryValues="@array/pref_developer_mdx_orbit_app_version_values" />
