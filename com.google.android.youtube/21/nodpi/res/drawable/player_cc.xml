<?xml version="1.0" encoding="utf-8"?>
<selector android:constantSize="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <bitmap android:src="@drawable/quantum_ic_closed_caption_white_48" />
    </item>
    <item android:state_enabled="true" android:state_selected="true">
        <bitmap android:src="@drawable/quantum_ic_closed_caption_white_48" />
    </item>
    <item android:state_enabled="true" android:state_selected="false">
        <bitmap android:src="@drawable/quantum_ic_closed_caption_white_48" android:alpha="@dimen/overflow_buttons_alpha_enabled" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/quantum_ic_closed_caption_white_48" android:alpha="@dimen/overflow_buttons_alpha_disabled" />
    </item>
</selector>