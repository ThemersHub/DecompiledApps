<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/live_chat_dialog_button_style_primary_color" />
            <corners android:radius="@dimen/live_chat_dialog_action_button_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/live_chat_dialog_button_style_primary_pressed_color" />
            <corners android:radius="@dimen/live_chat_dialog_action_button_radius" />
        </shape>
    </item>
</selector>