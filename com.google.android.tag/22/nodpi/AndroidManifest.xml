<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.android.apps.tag" platformBuildVersionCode="22" platformBuildVersionName="5.1.1-1863243">
